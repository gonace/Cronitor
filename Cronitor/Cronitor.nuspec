        <dependency id="Microsoft.Extensions.Hosting.Abstractions" version="9.0.10" />
        <dependency id="System.Text.Json" version="9.0.10" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\README.md" target="assets\" />
    <file src="..\assets\icon.png" target="assets\" />
    <file src=".\bin\Release\net9.0\*" target="lib\net9.0\" />
    <file src=".\bin\Release\net8.0\*" target="lib\net8.0\" />
    <file src=".\bin\Release\netstandard2.1\*" target="lib\netstandard2.1\" />
    <file src=".\bin\Release\netstandard2.0\*" target="lib\netstandard2.0\" />
  </files>
</package>
