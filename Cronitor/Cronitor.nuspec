    <summary>NET Client for Activity, Monitor, Notifications and Telemetry API...</summary>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/gonace/cronitor</projectUrl>
    <icon>assets\icon.png</icon>
    <readme>assets\README.md</readme>
    <repository type="git" url="https://github.com/gonace/cronitor" branch="master" />
