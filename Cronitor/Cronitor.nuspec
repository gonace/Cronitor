<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Cronitor</id>
    <title>Cronitor</title>
    <version>$version$</version>
    <authors>gonace,laget.se</authors>
    <owners>laget.se</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>NET Client for Activity, Monitor, Notifications and Telemetry API...</description>
    <summary></summary>
    <license type="expression">Apache-2.0</license>
    <projectUrl>https://laget.se</projectUrl>
    <icon>assets\icon.png</icon>
    <repository type="git" url="https://github.com/laget-se/Cronitor" branch="master" />
    <dependencies>
      <group targetFramework="netstandard2.1">
        <dependency id="Newtonsoft.Json" version="12.0.3" />
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="Newtonsoft.Json" version="12.0.3" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\assets\icon.png" target="assets\" />
    <file src=".\bin\Release\netstandard2.1\*" target="lib\netstandard2.1\" />
    <file src=".\bin\Release\netstandard2.0\*" target="lib\netstandard2.0\" />
